{
  "name": "threeium-sdk",
  "version": "0.1.0",
  "description": "Threeium SDK: Solana-native, production-grade TypeScript SDK for execution, liquidity, and yield accounting.",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.test.json --noEmit"
  },
  "dependencies": {
    "@solana/web3.js": "1.98.4"
  },
  "devDependencies": {
    "@types/node": "24.2.0",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  }
}
